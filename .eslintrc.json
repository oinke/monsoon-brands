{
    "extends": "google",
    "parser": "babel-eslint",
    "parserOptions": {
        "parser": "babel-eslint",
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "env": {
        "browser": true
    },
    "rules": {
        "new-cap": ["error", { "capIsNewExceptions": ["ReduxMixin"] }],
        "require-jsdoc": "off",
        "max-len": "off"
    },
    "globals": {

    }
}